<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="report_product_template_rocafer" model="ir.actions.report">
            <field name="name">Rocafer product print</field>
            <field name="model">product.template</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">rocafer_product_template_report.report_product_rocafer</field>
            <field name="report_file">rocafer_product_template_report.report_product_rocafer</field>
            <field name="print_report_name">'Rocafer product - %s' % (object.name)</field>
            <field name="binding_model_id" ref="product.model_product_template"/>
            <field name="binding_type">report</field>
        </record>

        <record id="report_order_product_template_rocafer" model="ir.actions.report">
            <field name="name">Rocafer order print</field>
            <field name="model">sale.order</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">rocafer_product_template_report.sale_order_rocafer</field>
            <field name="report_file">rocafer_product_template_report.sale_order_rocafer</field>
            <field name="print_report_name">'Rocafer order - %s' % (object.name)</field>
            <field name="binding_model_id" ref="sale.model_sale_order"/>
            <field name="binding_type">report</field>
        </record>

    </data>
</odoo>
