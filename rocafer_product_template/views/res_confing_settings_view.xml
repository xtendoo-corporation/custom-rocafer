<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="res_confing_settings_view_form" model="ir.ui.view">
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <block name="product_setting_container" position="before">
                <block title="Product Rocafer" name="product_rocafer">
                    <setting help="Default stand separation">
                        <field name="h1_value_field" class="oe_inline"/>
                    </setting>
                    <setting help="Coastal default measurement">
                        <field name="h2_value_field" class="oe_inline"/>
                    </setting>
                </block>
            </block>
        </field>
    </record>
</odoo>
